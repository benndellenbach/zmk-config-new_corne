/dts-v1/;

/* SoC */
#include <nordic/nrf52840_qiaa.dtsi>

/* Shared keyboard defs (NO USB inside) */
#include "eyelash_corne.dtsi"

/ {
    model = "Eyelash Corne Dongle";
    compatible = "eyelash,corne-dongle";

    zmk_split_central: split_central {
        compatible = "zmk,split-central";
    };

    chosen {
        zmk,central = &zmk_split_central;
    };
};

/* REQUIRED for studio-rpc-usb-uart */
zephyr_udc0: &usbd {
    status = "okay";
};
